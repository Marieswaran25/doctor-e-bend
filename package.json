{
    "name": "doctor-e-bend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "local": "set NODE_ENV=local&& nodemon",
        "dev": "set NODE_ENV=testing&& nodemon",
        "testing": "cross-env NODE_ENV=testing node dist/index.js",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format:check": "prettier --check \"{src,scripts,__tests__,.}/**/*.{ts,tsx}\" \"**/*.ts\"  \"**/*.json\" \"README.md\" ",
        "format:fix": "prettier --write \"{src,scripts,__tests__,.}/**/*.{ts,tsx}\" \"**/*.ts\"  \"**/*.json\" \"README.md\" ",
        "typeorm": "set NODE_ENV=testing&& ts-node ./node_modules/typeorm/cli -d src/dbConfig.ts",
        "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/%npm_config_name%",
        "migration:run": "npm run typeorm -- migration:run",
        "migration:revert": "npm run typeorm -- migration:revert"
    },
    "author": "Marieswaran",
    "license": "ISC",
    "dependencies": {
        "@godaddy/terminus": "^4.12.1",
        "axios": "^1.6.7",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.4.0",
        "express-validator": "^7.0.1",
        "handlebars": "^4.7.8",
        "helmet": "^7.1.0",
        "moment": "^2.30.1",
        "nodemon": "^3.0.3",
        "pg": "^8.16.0",
        "prettier": "^3.2.4",
        "puppeteer": "^20.9.0",
        "stripe": "^17.6.0",
        "ts-node": "^10.9.2",
        "typeorm": "^0.3.19",
        "typescript": "^5.3.3",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.11.5",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "eslint": "^8.56.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-simple-import-sort": "^10.0.0"
    }
}
